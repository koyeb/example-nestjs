				{
					"name": "Login User",
					"event": [
						{
							"listen": "test",
							"script": {
								"exec": [
									"pm.test(\"Access token save in env\", function () {",
									"    const jsonData = pm.response.json();",
									"    if (jsonData.access_token) {",
									"        pm.environment.set(\"access_token\", jsonData.access_token);",
									"    } else {",
									"        console.log(\"access_token is missing from the response.\");",
									"    }",
									"});",
									""
								],
								"type": "text/javascript",
								"packages": {}
							}
						},
						{
							"listen": "prerequest",
							"script": {
